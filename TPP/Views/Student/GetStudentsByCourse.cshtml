@{
    @model IEnumerable<Student>

}


@{
    if (Model.Count() <= 0)
    {
        <h1 class="alert alert-warning"> No Student in this course</h1>
    }
    else

    {
        <h1 style="color:DodgerBlue;">
                    <center>

                @ViewBag.Id

                    </center>

        </h1>
        <table class="table table-dark">
                    <thead>
                        <tr>
                            <th scope="col">Id</th>
                            <th scope="col">First Name</th>
                            <th scope="col">Last Name</th>
                            <th scope="col">Phone Number</th>
                            <th scope="col">University</th>
                            <th scope="col">Timestamp</th>
                        </tr>
                    </thead>
                    <tbody>
                    <tbody>
                    @foreach (Student c in Model)
                    {
                                <tr>
                                    <th scope="row">
                                @c.id
                                    </th>
                                    <td>
                                @c.first_name
                                    </td>
                                    <td>
                                @c.last_name
                                    </td>
                                    <td>
                                @c.phone_number
                                    </td>
                                    <td>
                                @c.university
                                    </td>
                                    <td>
                                @c.timestamp
                                    </td>
                                </tr>
                    }
                    </tbody>
        </table>
    }


}